<h1>Edit Instagram</h1>

<%= form_with scope: :insta, model: @insta, local: true do |form| %>


<p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

<p>
    <%= form.label :url %><br>
    <%= form.text_field :url %>
  </p>


  <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>

  

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', root_path %>